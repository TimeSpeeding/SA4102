@using MyApp2.Models;


<table width="400">
    <tr>
        <th>Product</th>
        <th>Brand</th>
        <th>Price (S$)</th>
    </tr>
    @foreach (var item in ViewData["transact"] as IList<TransactModel>)
    {
        <tr>
            <td>@item.Product</td>
            <td>@item.Brand</td>
            <td>@item.Price</td>
        </tr>
    }
</table>